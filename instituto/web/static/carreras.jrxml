	<queryString language="SQL">
		<![CDATA[SELECT carreras.nombre,
	carreras.carrera_id,
	carreras.descripcion,
	carreras.duracion,
	count(alumnos.carrera_id) AS "cantidad"
FROM carreras
	LEFT JOIN alumnos ON 
	 carreras.carrera_id = alumnos.carrera_id 
GROUP BY carreras.carrera_id]]>
